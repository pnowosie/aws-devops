STACK="website"
TEMPLATE="website"
PARAMETERS="website"

aws cloudformation describe-stacks --stack-name website-export-import-website-dev  --region eu-west-1 --output text --query "Stacks[].Outputs[?OutputKey=='S3BucketName'].OutputValue"
